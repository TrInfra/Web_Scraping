{
    "services": {
      "app": {
        "environment": {
          "MINIO_ROOT_USER": "${MINIO_ROOT_USER}",
          "MINIO_ROOT_PASSWORD": "${MINIO_ROOT_PASSWORD}"
        },
        "ports": [
          "5000:5000"
        ],
        "volumes": [
          ".:/app"
        ],
        "command": [
          "/opt/venv/bin/python",
          "app.py"
        ],
        "depends_on": [
          "minio"
        ]
      },
      "minio": {
        "image": "minio/minio",
        "environment": {
          "MINIO_ROOT_USER": "${MINIO_ROOT_USER}",
          "MINIO_ROOT_PASSWORD": "${MINIO_ROOT_PASSWORD}"
        },
        "ports": [
          "9000:9000",
          "9090:9090"
        ],
        "volumes": [
          "minio_data:/data"
        ],
        "command": [
          "server",
          "/data",
          "--console-address",
          ":9090"
        ]
      }
    },
    "volumes": {
      "minio_data": {
        "driver": "local"
      }
    }
  }